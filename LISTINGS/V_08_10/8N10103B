
8-bit byte:          cl = val;
                     asm(ebx, cl, "     mov    byte ptr es:[ebx], cl");

16-bit word:         cx = val;
                     asm(ebx, cx, "     mov    word ptr es:[ebx], cx");

32-bit double word:  ecx = val;
                     asm(ebx, ecx, "     mov    dword ptr es:[ebx], ecx");

