




             struct _DDEINIT
             {
                 USHORT            cb;
                 PSZ               pszApplName;
                 PSZ               pszTopic;
             };
             typedef struct _DDEINIT            DDEINIT;



             struct _DDESTRUCT
             {
                 ULONG             cbData;
                 USHORT            usStatus;
                 USHORT            usFormat;
                 USHORT            offszItemName;
                 USHORT            offabData;
             };
             typedef struct _DDESTRUCT         DDESTRUCT;


