/* SOURCE:  INFORMIX 4GL REFERENCE MANUAL, VOL I, PAGE 1-59 */

void popint(int *);             /* Popping Functions */
void popshort(short *);
void poplong(long *);
void popflo(float *);
void popdub(double *);
void popquote(char *, short);
void popdec(struct dec_t *);

void retint(int);               /* Pushing Functions */
void retshort(short);
void retlong(long);
void retflo(float *);
void retdub(double *);
void retquote(char *);
void retdec(struct dec_t *);
