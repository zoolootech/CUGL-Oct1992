
    XKeyEvent       *event;
    char            *string;        /* RETURNED */
    int             max_string_length;
    KeySym          *keysym;
    XComposeStatus  *composestatus;
    int             returned_length;

    returned_length = XLookupString( event,
                    string, max_string_length,
                    keysym, composestatus );

