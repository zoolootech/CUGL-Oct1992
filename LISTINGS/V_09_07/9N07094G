
    typedef struct
        {
        int             type;   /* KeyPress */
        unsigned long   serial;
        Bool            send_event;
        Display         *display;
        Window          window; 
        Window          root;
        Window          subwindow;
        Time            time;
        int             x, y;
        int             x_root, y_root;
        unsigned int    state;  /* Shift, Ctrl, etc. */
        unsigned int    keycode;    /* key pressed */
        Bool            same_screen;
        } XKeyEvent;

