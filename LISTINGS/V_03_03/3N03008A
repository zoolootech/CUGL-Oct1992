typedef struct {
    WORD    wType;
    WORD    wWidth;
    WORD    wHeight;
    DWORD   dwStyle;
    char    szDescr[CTLDESCR];
} CTLTYPE;

typedef struct {
    WORD            wVersion;
    WORD            wCtlTypes;
    char            szClass[CTLCLASS];
    char            szTitle[CTLTITLE];
    char            szReserved[10];
    CTLTYPE         Type[CTLTYPES];
} CTLINFO;

