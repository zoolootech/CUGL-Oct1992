switch(wParam)
{
/* ... */
   case PROGRAMMER:
     ProgrammerRoutine();       /* Explicit call to programmer
     break;                      * defined code here serves to
                                 * override default code in the
                                 * .WMC file
                                 */
/*...*/
   default:
     BLDDefDialogProc();        /* Call default routine in .WMC
                                 * file here
                                 */
     break;
}

