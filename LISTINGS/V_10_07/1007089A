
main()
        {
        int c;
        
        while (c = nextsp())
                putchar(c);
        while (c = nextap())
                putchar(c);
                
        }

nextsp()
        {
        static char *p = "This string will be shown from the beginning to the end";
        return *p++;
        }

nextap()
        {
        char *p = "This string will never be shown";
        return *p++;
        }
