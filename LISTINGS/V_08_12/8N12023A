
typedef unsigned char TenByteReal[10];

typedef struct {
    unsigned  j_sp;
    unsigned  j_ss;
    unsigned  j_flag;
    unsigned  j_cs;
    unsigned  j_ip;
    unsigned  j_bp;
    unsigned  j_di;
    unsigned  j_es;
    unsigned  j_si;
    unsigned  j_ds;
    TenByteReal st1, st2;
} jmp_buf[1];

