
#include "stdio.h"

int addup(int a, int b);

int main()
    {
    int a
    int b
    int c;
    
    for (a = 0; a < 10; ++a)
        {
        for (b = 0; b < 10; ++b)
            {
            c = addup(a,b);
            printf("%i + %i = %i\n",a,b,c);
            }
        }
    
    return 0;
    }

int addup(int a, int b)
    {
    return (a + b);
    }

