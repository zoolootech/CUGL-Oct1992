
     MAIN_FILE defined                  

     int  foo;                          
     WORD bar = 5;                      
     BYTE aaa[5] = {1,2,3,4,5};         
     BYTE tbl[] = { #include table.h }; 

     MAIN_FILE not defined      
                           
     extern int foo;            
     extern WORD bar;           
     extern BYTE aaa[5];        
     extern BYTE tbl[];         

