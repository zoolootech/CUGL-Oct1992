/*
	listing 5 - lookup_print.c
*/

#include <stdio.h>
#include "common.h"

lookup_print (struct record *tran_start)
{

  printf ("id     = %5d, ", tran_start->id);

  printf ("number = %5d, ", tran_start->number);

  printf ("price  = %5.2f, ", tran_start->price);

  printf ("code   = %5s\n", tran_start->code);

  printf ("\n\n");

}
