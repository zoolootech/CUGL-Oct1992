
        TYPE EmployeeStruc 
                ID          AS STRING * 12      ' employee id 
                SSNumber    AS STRING * 9       ' social security number 
                LastName    AS STRING * 25      ' last name 
                FirstName   AS STRING * 15      ' first name 
                MiddleInit  AS STRING * 1       ' middle initial 
                StartDate   AS STRING * 8       ' starting date: mmddyyyy 
                Age         AS INTEGER          ' current age 
        END TYPE 

