     
    #include <stdio.h>
    #include <math.h>
    #include <ctype.h>
    
    void main()
    {
     double x,y ;
    
     for ( x = 1.0; x <= 25.0; (x = x + 1.0) )
      {
       y = sqrt(x) * sqrt(x);
       if (y != x)
         printf("%5.1f %21.17f %10.2e\n", x, y, x-y);
      }
    }
    
    LISTING 4

