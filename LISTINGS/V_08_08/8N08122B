    
    
    #include <stdio.h>
    #include <ctype.h>
    #include <math.h>
    
    void main()
    {
     double f;
     int index, n = 75;
    
     f = 1.0 / 10000.0 ;
     f = f / 3.0;
     for(index = 1; index < n; index++)
      {
       f = f / 10.0;
       printf(" %-2d %e\n", index, f);
      }
    }
     
    LISTING 2

