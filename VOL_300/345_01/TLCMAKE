	C {directives} tlcresw.c
tlcsetp.c.o Ÿ tlc.make tlcsetp.c {tlcincs}
	C {directives} tlcsetp.c
tlcstat.c.o Ÿ tlc.make tlcstat.c {tlcincs}
	C {directives} tlcstat.c
tlcutil.c.o Ÿ tlc.make tlcutil.c {tlcincs}
	C {directives} tlcutil.c
tlcxref.c.o Ÿ tlc.make tlcxref.c {tlcincs}
	C {directives} tlcxref.c
tlc ŸŸ tlc.make ë
