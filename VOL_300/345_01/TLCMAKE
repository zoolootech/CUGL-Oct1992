tlcincs		= tlc.h tlc.ext
directives	= -mbg 15

tlc.c.o Ÿ tlc.make tlc.c {tlcincs}
	C {directives} tlc.c
tlccmd.c.o Ÿ tlc.make tlccmd.c {tlcincs}
	C {directives} tlccmd.c
tlcgtok.c.o Ÿ tlc.make tlcgtok.c {tlcincs}
	C {directives} tlcgtok.c
